{
  "name": "mapnj",
  "version": "0.2.0",
  "description": "A script to add interactivity (such as click events) to SVG-based illustration maps",
  "main": "lib/index.js",
  "browser": "dist/MapNJ.min.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc && webpack",
    "watch": "tsc-watch --onSuccess \"npm run build\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "lib/**/*",
    "dist/**/*",
    "README.md"
  ],
  "keywords": [
    "svg",
    "map",
    "interactive",
    "illustration",
    "click-events"
  ],
  "author": "Masaaki Sumimoto <masa.sumimoto@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/masa-sumimoto/mapnj.git"
  },
  "bugs": {
    "url": "https://github.com/masa-sumimoto/mapnj/issues"
  },
  "homepage": "https://github.com/masa-sumimoto/mapnj#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.2.2",
    "ts-loader": "^9.5.1",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.5.3",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  }
}
